import run.GUI;

Plant(
		input signal ConveyorON, ConveyorOFF;
		output signal ConveyorONe, ConveyorOFFe;
		)
->
{
	new Thread(new GUI()).start();

	{	
		while(true) {
			await(ConveyorON);
			{
				System.out.println("ConveyorON");
				emit ConveyorONe;
				
			}
			await(ConveyorOFF);
			{
				System.out.println("Conveyor OFF");
				emit ConveyorOFFe;
				
			}
			
			
		}
		
	}
	
}
